{
  "$defs": {
    "OptimizerConfig": {
      "properties": {
        "model": {
          "title": "Model",
          "type": "object"
        }
      },
      "required": [
        "model"
      ],
      "title": "OptimizerConfig",
      "type": "object"
    },
    "PromptConfig": {
      "properties": {
        "identifier": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Identifier"
        },
        "prompt_str": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Prompt Str"
        },
        "model_config": {
          "anyOf": [
            {
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Model Config"
        },
        "which": {
          "default": 0,
          "title": "Which",
          "type": "integer"
        }
      },
      "title": "PromptConfig",
      "type": "object"
    }
  },
  "properties": {
    "name": {
      "title": "Name",
      "type": "string"
    },
    "description": {
      "title": "Description",
      "type": "string"
    },
    "evaluator_descriptions": {
      "title": "Evaluator Descriptions",
      "type": "object"
    },
    "dataset": {
      "title": "Dataset",
      "type": "string"
    },
    "initial_prompt": {
      "$ref": "#/$defs/PromptConfig"
    },
    "baseline_experiment": {
      "anyOf": [
        {
          "format": "uuid",
          "type": "string"
        },
        {
          "type": "null"
        }
