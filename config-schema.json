    },
    "optimizer": {
      "anyOf": [
        {
          "$ref": "#/$defs/OptimizerConfig"
        },
        {
          "type": "null"
        }
      ]
    },
    "evaluators": {
      "title": "Evaluators",
      "type": "string"
    },
    "system": {
      "title": "System",
      "type": "string"
    }
  },
  "required": [
    "name",
    "description",
    "evaluator_descriptions",
    "dataset",
    "initial_prompt",
    "baseline_experiment",
    "optimizer",
    "evaluators",
    "system"
  ],
  "title": "Config",
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema#"
